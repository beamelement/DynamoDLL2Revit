<Window  Name="window"
    x:Class="Dynamo2Revit.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dynamo2Revit"
        mc:Ignorable="d"
        Title="选择一个空的dynamo文件" Height="150" Width="400">
    <Grid>

        <Grid  >
            <Grid.RowDefinitions>
                <RowDefinition Height="50"  />
                <RowDefinition Height="50"/>
            </Grid.RowDefinitions>

            <StackPanel Orientation="Horizontal" Grid.Row="0"  HorizontalAlignment="Right">
                <TextBlock Text="请选择一个空的dynamo文件"  FontSize="20" VerticalAlignment="Center"  HorizontalAlignment="Right" Height="25" Width="260" Margin="0,13" />
                <Button Content="选择"  Width="60" Height="30"   Margin="50 0 5 0" Click="selection" />
            </StackPanel>


            <Button Grid.Row="2" Content="确定"  Margin="0,10,10,5" Click="DoneClick"  HorizontalAlignment="Right" Width="350"/>
        </Grid>

    </Grid>
</Window>
